{
  "permissions": {
    "allow": [
      "Bash(done)",
      "Bash(find:*)",
      "Bash(grep:*)",
      "Bash(head:*)",
      "Bash(ls:*)",
      "Bash(git init:*)",
      "Bash(git diff:*)",
      "Bash(git add:*)",
      "Bash(git commit:*)",
      "Bash(npm install:*)",
      "Bash(npm run lint:*)",
      "WebSearch",
      "WebFetch(domain:github.com)"
    ],
    "deny": [
      "Bash(git push:*)",
      "Bash(rm -rf:*)"
    ]
  },
  "enabledPlugins": {
    "frontend-design@claude-plugins-official": true,
    "github@claude-plugins-official": true,
    "figma@claude-plugins-official": true,
    "slack@claude-plugins-official": true,
    "gopls-lsp@claude-plugins-official": true
  },
  "attribution": {
    "commit": "",
    "pr": ""
  }
}
